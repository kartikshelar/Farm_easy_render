{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.8"
      },
      "env": {
        "PYTHONPATH": "$PYTHONPATH:/vercel/path0/lib/python3.8/site-packages"
      },
      "commands": {
        "install": "apt-get update && apt-get install -y libpq-dev"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/app.py"
    }
  ],
  "env": {
    "FLASK_APP": "app.py",
